{
    "name": "sota/system",
    "description": "The Sota System Laravel Package",
    "license": "None",
    "authors": [
        {
            "name": "Sota Dev Group"
        }
    ],
    "homepage": "https://github.com/sotadevgroup/sota-system",
    "keywords": ["Sota", "System", "Laravel"],
    "require": {
        "php": "^7.1",
        "erjanmx/laravel-migrate-check": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lorisleiva/laravel-deployer": "dev-master",
        "sentry/sentry-laravel": "^0.9.2",
        "spatie/laravel-backup": "^5.10",
        "spatie/laravel-json-api-paginate": "^1.4",
        "spatie/laravel-query-builder": "^1.10",
        "tymon/jwt-auth": "1.0.0-rc.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Sota\\System\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sota\\System\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sota\\System\\SystemServiceProvider"
            ]
        }
    }
}
